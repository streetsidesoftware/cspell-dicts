{
  "name": "@cspell/dict-scala",
  "version": "5.0.3",
  "description": "Scala dictionary for cspell.",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./cspell-ext.json",
    "./cspell": "./cspell-ext.json",
    "./cspell-ext.json": "./cspell-ext.json"
  },
  "scripts": {
    "build": "cspell-tools-cli build",
    "test": "shx head -n 1000 src/scala.txt | cspell -c ./cspell-ext.json --locale=* --languageId=scala stdin",
    "prepublishOnly": "echo pre-publish",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/streetsidesoftware/cspell-dict.git",
    "directory": "dictionaries/scala"
  },
  "keywords": [
    "cspell",
    "scala",
    "Scala",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software <support@streetsidesoftware.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/streetsidesoftware/cspell-dicts/issues"
  },
  "homepage": "https://github.com/streetsidesoftware/cspell-dicts/blob/main/dictionaries/scala#readme",
  "files": [
    "dict/scala.txt",
    "cspell-ext.json"
  ]
}
