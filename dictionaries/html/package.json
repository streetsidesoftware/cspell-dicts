{
  "name": "@cspell/dict-html",
  "version": "4.0.6",
  "description": "HTML dictionary for cspell.",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./cspell-ext.json",
    "./cspell": "./cspell-ext.json",
    "./cspell-ext.json": "./cspell-ext.json"
  },
  "scripts": {
    "build": "cspell-tools-cli build",
    "test": "shx head -n 100 src/html.txt | cspell -c ./cspell-ext.json \"--locale=*\" --language-id=html stdin",
    "prepublishOnly": "echo pre-publish",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/streetsidesoftware/cspell-dict.git",
    "directory": "dictionaries/html"
  },
  "keywords": [
    "cspell",
    "html",
    "html",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software <support@streetsidesoftware.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/streetsidesoftware/cspell-dicts/issues"
  },
  "homepage": "https://github.com/streetsidesoftware/cspell-dicts/blob/main/dictionaries/html#readme",
  "files": [
    "dict/html.txt",
    "cspell-ext.json"
  ]
}
