{
  "name": "@cspell/dict-clojure",
  "version": "2.0.1",
  "description": "Clojure dictionary for cspell.",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./cspell-ext.json",
    "./cspell": "./cspell-ext.json",
    "./cspell-ext.json": "./cspell-ext.json"
  },
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=8192 cspell-tools-cli compile \"src/clojure.txt\" -o .",
    "test": "head -n 1000 \"src/clojure.txt\" | cspell -v -c ./cspell-ext.json \"--local=en\" \"--languageId=clojure\" stdin",
    "prepublishOnly": "echo pre-publish",
    "prepare": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streetsidesoftware/cspell-dicts.git"
  },
  "keywords": [
    "cspell",
    "cspell-ext",
    "clojure",
    "clojure",
    "dictionary",
    "spelling"
  ],
  "author": "Jason Dent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/streetsidesoftware/cspell-dicts/issues"
  },
  "homepage": "https://github.com/streetsidesoftware/cspell-dicts/blob/main/dictionaries/clojure#readme",
  "files": [
    "clojure.txt.gz",
    "cspell-ext.json",
    "*.js",
    "*.d.ts"
  ]
}
