{
    "version": "0.2",
    "files": [
        "**/*.{md,txt}"
    ],
    "import": [
        "./cspell-ext.json"
    ],
    "ignorePaths": [
        "src/hunspell"
    ],
    "ignoreWords": [
    ],
    "overrides": [
        {
            "filename": "samples/**/*.md",
            "caseSensitive": true,
            "dictionaries": [
                "da-dk"
            ]
        },
        {
            "filename": "*.json",
            "ignoreRegExpList": ["/\"(alphabet|map)\":.*/"]
        }
    ]
}
