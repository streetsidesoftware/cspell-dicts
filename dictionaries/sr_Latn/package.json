{
  "name": "@cspell/dict-sr-latn",
  "version": "1.0.2",
  "description": "Serbian (Latin) dictionary for cspell.",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./cspell-ext.json",
    "./cspell": "./cspell-ext.json",
    "./cspell-ext.json": "./cspell-ext.json"
  },
  "scripts": {
    "build": "cspell-tools-cli build",
    "conditional-build": "pnpm run build --conditional",
    "prepublishOnly": "echo pre-publish",
    "test": "cspell -v -c ./cspell-ext.json --locale=sr-Latn \"samples/**\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streetsidesoftware/cspell-dicts.git"
  },
  "keywords": [
    "cspell",
    "cspell-ext",
    "sr_Latn",
    "Serbian (Latin)",
    "dictionary",
    "spelling"
  ],
  "author": "Jason Dent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/streetsidesoftware/cspell-dicts/issues"
  },
  "homepage": "https://github.com/streetsidesoftware/cspell-dicts/blob/main/dictionaries/sr_Latn#readme",
  "devDependencies": {},
  "dependencies": {},
  "files": [
    "dict/sr-latn.trie",
    "cspell-ext.json",
    "*.js",
    "*.d.ts"
  ]
}
