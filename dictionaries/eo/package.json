{
  "name": "@cspell/dict-eo",
  "version": "2.0.0",
  "description": "Esperanto dictionary for cspell.",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./cspell-ext.json",
    "./cspell": "./cspell-ext.json",
    "./cspell-ext.json": "./cspell-ext.json"
  },
  "scripts": {
    "build": "cspell-tools-cli build && pnpm gz",
    "test": "hunspell-reader words -n 1000 \"src/hunspell/eo.dic\" | cspell -v -c ./cspell-ext.json \"--local=eo\" \"--languageId=*\" stdin",
    "gz": "cspell-tools-cli gzip *.trie",
    "prepare": "pnpm gz",
    "prepublishOnly": "echo pre-publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streetsidesoftware/cspell-dicts.git"
  },
  "keywords": [
    "cspell",
    "eo",
    "Esperanto",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software",
  "license": "GPL-3.0+",
  "bugs": {
    "url": "https://github.com/streetsidesoftware/cspell-dicts/issues"
  },
  "homepage": "https://github.com/streetsidesoftware/cspell-dicts/blob/main/dictionaries/eo#readme",
  "files": [
    "eo.trie.gz",
    "src/hunspell/eo_license.txt",
    "cspell-ext.json"
  ]
}
