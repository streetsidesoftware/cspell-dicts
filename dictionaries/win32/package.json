{
  "name": "@cspell/dict-win32",
  "version": "1.0.1",
  "description": "Win32 dictionary for cspell.",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./cspell-ext.json",
    "./cspell": "./cspell-ext.json",
    "./cspell-ext.json": "./cspell-ext.json"
  },
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=8192 cspell-tools-cli compile --keep-raw-case \"src/win32.txt\" -o .",
    "test": "yarn run test-src && yarn run test-samples",
    "test-src": "head -n 10000 \"src/win32.txt\" | cspell -v -c ./cspell.json \"--local=*\" stdin",
    "test-samples": "cspell samples/**",
    "prepublishOnly": "yarn test",
    "prepare": "yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streetsidesoftware/cspell-dicts.git"
  },
  "keywords": [
    "cspell",
    "cspell-ext",
    "Win32",
    "dictionary",
    "spelling"
  ],
  "author": "Jason Dent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/streetsidesoftware/cspell-dicts/issues"
  },
  "homepage": "https://github.com/streetsidesoftware/cspell-dicts/blob/main/packages/Win32#readme",
  "files": [
    "win32.txt.gz",
    "cspell-ext.json",
    "*.js",
    "*.d.ts"
  ]
}
