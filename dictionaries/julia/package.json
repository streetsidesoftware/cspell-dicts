{
  "name": "@cspell/dict-julia",
  "version": "1.0.0",
  "description": "Julia dictionary for cspell.",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./cspell-ext.json",
    "./cspell": "./cspell-ext.json",
    "./cspell-ext.json": "./cspell-ext.json"
  },
  "scripts": {
    "build": "cspell-tools-cli compile --keep-raw-case src/julia.txt --no-compress -o ./dict",
    "test": "shx head -n 1000 src/julia.txt | cspell -v -c ./cspell-ext.json --locale=* --languageId=julia stdin",
    "prepublishOnly": "echo pre-publish",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/streetsidesoftware/cspell-dict.git",
    "directory": "dictionaries/julia"
  },
  "keywords": [
    "cspell",
    "julia",
    "Julia",
    "julialang",
    "dictionary",
    "spelling"
  ],
  "author": "Th√©o Galy-Fajou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/streetsidesoftware/cspell-dicts/issues"
  },
  "homepage": "https://github.com/streetsidesoftware/cspell-dicts/blob/main/dictionaries/julia#readme",
  "files": [
    "dict/julia.txt",
    "dict/julia-keywords.txt",
    "cspell-ext.json"
  ]
}
