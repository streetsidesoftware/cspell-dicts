{
  "name": "@cspell/dict-sr-cyrl",
  "version": "1.1.0",
  "description": "Serbian (Cyrillic) dictionary for cspell.",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./cspell-ext.json",
    "./cspell": "./cspell-ext.json",
    "./cspell-ext.json": "./cspell-ext.json"
  },
  "scripts": {
    "build": "cspell-tools-cli build",
    "conditional-build": "pnpm run build --conditional",
    "prepublishOnly": "echo pre-publish",
    "test": "cspell -v -c ./cspell-ext.json --local=sr-Cyrl \"samples/**\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streetsidesoftware/cspell-dicts.git"
  },
  "keywords": [
    "cspell",
    "cspell-ext",
    "sr_Cyrl",
    "Serbian (Cyrillic)",
    "dictionary",
    "spelling"
  ],
  "author": "Jason Dent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/streetsidesoftware/cspell-dicts/issues"
  },
  "homepage": "https://github.com/streetsidesoftware/cspell-dicts/blob/main/dictionaries/sr_Cyrl#readme",
  "devDependencies": {},
  "dependencies": {},
  "files": [
    "dict/sr-cyrl.trie",
    "cspell-ext.json",
    "*.js",
    "*.d.ts"
  ]
}
