{
  "id": "npm",
  "name": "NPM",
  "description": "Dictionary of Common Javascript Packages.",
  "readonly": true,
  "dictionaryDefinitions": [
    {
      "name": "npm",
      "path": "./dict/npm.txt",
      "description": "Dictionary of Common Javascript Packages."
    }
  ],
  "languageSettings": [
    {
      "languageId": "javascript,typescript,javascriptreact,typescriptreact",
      "locale": "*",
      "dictionaries": ["npm"]
    }
  ],
  "ignoreRegExpList": ["npm-scripts"],
  "patterns": [
    {
      "name": "npm-scripts",
      "pattern": ["npm-scripts-pre", "npm-scripts-post"],
      "description": "Matches NPM scripts pre and post from NPM lifecycle."
    },
    {
      "name": "npm-scripts-pre",
      "pattern": "/pre[^\"]+/",
      "description": "Matches NPM scripts pre from NPM lifecycle."
    },
    {
      "name": "npm-scripts-post",
      "pattern": "/post[^\"]+/",
      "description": "Matches NPM scripts post from NPM lifecycle."
    }
  ],
  "overrides": [
    {
      "filename": "**/package.json",
      "dictionaries": ["npm"]
    }
  ]
}
