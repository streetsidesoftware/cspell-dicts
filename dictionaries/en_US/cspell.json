{
    "version": "0.2",
    "files": [
        "**/*.{md,txt}"
    ],
    "language": "en-us",
    "import": [
        "./cspell-ext.json"
    ],
    "ignorePaths": [
        "/src/legacy/**",
        "/src/hunspell/**",
        "/cspell-ext.json",
        "CHANGELOG.md"
    ],
    "ignoreWords": [],
    "overrides": [
        {
            "filename": "/src/{legacy,hunspell}/**",
            "enabled": false
        }
    ],
    "words": [
        "issuecomment",
        "shasum",
        "xargs"
    ]
}
