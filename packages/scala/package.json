{
  "name": "cspell-dict-scala",
  "version": "1.0.16",
  "description": "Scala dictionary for cspell.",
  "bin": {
    "cspell-dict-scala-link": "./link.js",
    "cspell-dict-scala-unlink": "./unlink.js"
  },
  "scripts": {
    "build": "cspell-tools compile \"scala.txt\" -o .",
    "prepublishOnly": "npm t",
    "prepare": "cp ../../util/* . && npm run build",
    "cspell-link": "node link.js",
    "cspell-unlink": "node unlink.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streetsidesoftware/cspell-dicts.git"
  },
  "keywords": [
    "cspell",
    "scala",
    "Scala",
    "dictionary",
    "spelling"
  ],
  "main": "index.js",
  "typings": "index.d.ts",
  "author": "Jason Dent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/streetsidesoftware/cspell-dicts/issues"
  },
  "homepage": "https://github.com/streetsidesoftware/cspell-dicts/blob/master/packages/scala#readme",
  "dependencies": {
    "configstore": "^5.0.1"
  },
  "files": [
    "scala.txt.gz",
    "cspell-ext.json",
    "*.js",
    "*.d.ts"
  ],
  "gitHead": "7d4bb142ab4989560dc3a187fdf3632109125f61"
}
