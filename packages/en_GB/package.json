{
  "name": "cspell-dict-en-gb",
  "version": "1.2.5",
  "description": "British English dictionary for cspell.",
  "bin": {
    "cspell-dict-en-gb-link": "link.js",
    "cspell-dict-en-gb-unlink": "unlink.js"
  },
  "exports": {
    ".": "./index.js",
    "./cspell-ext.json": "./cspell-ext.json"
  },
  "scripts": {
    "prepublishOnly": "yarn test",
    "test": "cspell -c cspell-ext.json *.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streetsidesoftware/cspell-dicts.git"
  },
  "keywords": [
    "cspell",
    "en_GB",
    "dictionary",
    "spelling"
  ],
  "main": "index.js",
  "typings": "index.d.ts",
  "author": "Jason Dent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/streetsidesoftware/cspell-dicts/issues"
  },
  "homepage": "https://github.com/streetsidesoftware/cspell-dicts/blob/main/packages/en_GB#readme",
  "dependencies": {
    "@cspell/dict-en-gb": "^1.1.32",
    "configstore": "^6.0.0"
  },
  "files": [
    "cspell-ext.json",
    "*.js",
    "*.d.ts"
  ],
  "directories": {
    "test": "tests"
  }
}
